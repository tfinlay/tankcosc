{
  "name": "tankcosc_cli",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "bin": "./bin/index.js",
  "scripts": {
    "prebuild": "tsc --noEmit",
    "build": "node ./build.mjs",
    "prepackage": "yarn build",
    "package": "pkg .",
    "devPackage": "yarn build && pkg ./bin/index.js -o devcli",
    "start": "yarn devPackage && devcli"
  },
  "pkg": {
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-win-x64"
    ],
    "assets": ["templates/**/*"],
    "outputPath": "dist"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "socket.io-client": "^4.4.1",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/node": "^17.0.23",
    "@types/source-map-support": "^0.5.4",
    "esbuild": "^0.13.7",
    "pkg": "^5.5.2",
    "typescript": "^4.4.4"
  }
}
